{
  "version": "0.2.0",
  "configurations": [

  
    {
      "type": "node",
      "request": "launch",
      "name": "Es6",
      "program": "${workspaceRoot}/src/start.js",
      "outFiles": ["${workspaceRoot}/app/**/*.js"],
      "runtimeExecutable": "babel-node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Current File",
      "program": "${file}",
      "outFiles": ["${workspaceRoot}/app/**/*.js"],
      "runtimeExecutable": "babel-node"
    }
  ]
}
